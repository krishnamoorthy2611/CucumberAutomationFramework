pipeline{
		agent any
			stages{
				stage ('Compile Stage'){
		
					steps{
						withMaven(maven : 'E:/Ecilpse/Maven/apache-maven-3.3.9-bin/apache-maven-3.3.9') {
						sh 'mvn clean compile'
						}
					}
		
					}
		stage ('Testing Stage'){
		steps{
				withMaven(maven : 'E:/Ecilpse/Maven/apache-maven-3.3.9-bin/apache-maven-3.3.9') {
					sh 'mvn test'
					}
				}
		
		}
		stage ('deployment Stage'){
		steps{
				withMaven(maven : 'E:/Ecilpse/Maven/apache-maven-3.3.9-bin/apache-maven-3.3.9') {
					sh 'mvn deploy'
					}
				}
		}
	}
	}